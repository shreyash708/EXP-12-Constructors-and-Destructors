#include <iostream>
using namespace std;
   int rollno;
    char name[50];
    double fee;
    public:
student()
    {
        cout<<"Enter the RollNo:";
        cin>> rollno;
        cout<<"Enter the Name:";
        cin>>name;
        cout<<"Enter the Fee:";         
        cin>>fee;      }   

void display()
{
        cout<<endl<< rollno <<"\t"<<name<<"\t"<<fee;
    }
};

int main()
{
    student s;        

s.display();
    return 0;
 }

    return 0;
}
